# test from nfsim/test/dimer

begin parameters

   ITERATIONS  500
   MCELL_DIFFUSION_CONSTANT_3D_A 1e-3
   MCELL_NO_COMPARTMENT_SIZE 1

   NA_um3 6.022e8
   VOL_RXN 1
   MCELL_REDEFINE_VOL_RXN NA_um3
   
   
   kp 5e8/6.022e8 * VOL_RXN
   km 1e4
end parameters

begin molecule types
   A(a)
end molecule types


begin seed species
  A(a) 1000
end seed species

begin reaction rules
  A(a) + A(a) <-> A(a!1).A(a!1) kp, km
end reaction rules

begin observables
  Molecules Aa A(a)
  Molecules Aa1Aa1 A(a!1).A(a!1)
end observables

generate_network({overwrite=>1})
simulate({method=>"ode",t_end=>1e-3,n_steps=>1000})
