begin model
begin parameters

	ITERATIONS  1000
    MCELL_DIFFUSION_CONSTANT_3D_anti 1e-3
    MCELL_DIFFUSION_CONSTANT_3D_rec 1e-3
    MCELL_DIFFUSION_CONSTANT_3D_B 1e-3

    MCELL_NO_COMPARTMENT_SIZE 1
    NA_um3 6.022e8
    VOL_RXN 1
    UNIMOL_RXN 10000
    MCELL_REDEFINE_VOL_RXN NA_um3
    
    kon1     10 * VOL_RXN
	koff1    1 * UNIMOL_RXN
	
    kon2     10 * VOL_RXN
	koff2    1 * UNIMOL_RXN

	koff3    5 * UNIMOL_RXN
	koff4    10 * UNIMOL_RXN
end parameters

begin species	
    anti(r,r)   1000
    rec(a,b,b)  2000
	B(c,p~U)    2000
end species

begin reaction rules	
	rec(b)+B(c)     <->  rec(b!1).B(c!1)       kon1, koff1
	rec(a)+anti(r)  <->  rec(a!1).anti(r!1)    kon2, koff2
end reaction rules

begin observables
    Species Bc1pUBc2pUantir3rreca3b1b2 B(c!1,p~U).B(c!2,p~U).anti(r!3,r).rec(a!3,b!1,b!2)
    Species Bc1pUBc2pUrecab1b2 B(c!1,p~U).B(c!2,p~U).rec(a,b!1,b!2)
    Species Bc1pUantir2r3reca3bbreca2b1b B(c!1,p~U).anti(r!2,r!3).rec(a!3,b,b).rec(a!2,b!1,b)
    Species Bc1pUantir2rreca2b1b B(c!1,p~U).anti(r!2,r).rec(a!2,b!1,b)
    Species Bc1pUrecab1b B(c!1,p~U).rec(a,b!1,b)
    Species BcpU B(c,p~U)
    Species antir1r2reca1bbreca2bb anti(r!1,r!2).rec(a!1,b,b).rec(a!2,b,b)
    Species antir1rreca1bb anti(r!1,r).rec(a!1,b,b)
    Species antirr anti(r,r)
    Species recabb rec(a,b,b)
    Species antir1r2reca1b3breca2b4bBc3pUBc4pU anti(r!1,r!2).rec(a!1,b!3,b).rec(a!2,b!4,b).B(c!3,p~U).B(c!4,p~U)
    Species antir1r2reca1bbreca2b3b4Bc3pUBc4pU anti(r!1,r!2).rec(a!1,b,b).rec(a!2,b!3,b!4).B(c!3,p~U).B(c!4,p~U)
    Species antir1r2reca2bbreca1b3bBc3pU anti(r!1,r!2).rec(a!2,b,b).rec(a!1,b!3,b).B(c!3,p~U)
    Species antir1rreca1b2b3Bc2pUBc3pU anti(r!1,r).rec(a!1,b!2,b!3).B(c!2,p~U).B(c!3,p~U)
    Species antir1rreca1b2bBc2pU anti(r!1,r).rec(a!1,b!2,b).B(c!2,p~U)
    Species recab1b2Bc1pUBc2pU rec(a,b!1,b!2).B(c!1,p~U).B(c!2,p~U)
    Species recab1bBc1pU rec(a,b!1,b).B(c!1,p~U)
    Species Bc1pUBc2pUBc3pUBc4pUantir5r6reca5b1b2reca6b3b4 B(c!1,p~U).B(c!2,p~U).B(c!3,p~U).B(c!4,p~U).anti(r!5,r!6).rec(a!5,b!1,b!2).rec(a!6,b!3,b!4)
    Species Bc1pUBc2pUBc3pUantir4r5reca4b1breca5b2b3 B(c!1,p~U).B(c!2,p~U).B(c!3,p~U).anti(r!4,r!5).rec(a!4,b!1,b).rec(a!5,b!2,b!3)
    Species Bc1pUBc2pUantir3r4reca3b1breca4b2b B(c!1,p~U).B(c!2,p~U).anti(r!3,r!4).rec(a!3,b!1,b).rec(a!4,b!2,b)
    Species Bc1pUBc2pUantir3r4reca3bbreca4b1b2 B(c!1,p~U).B(c!2,p~U).anti(r!3,r!4).rec(a!3,b,b).rec(a!4,b!1,b!2)
    
end observables
end model

generate_network({overwrite=>1})
simulate({method=>"ode",t_end=>1e-3,n_steps=>1000})
