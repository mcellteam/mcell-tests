begin model
begin parameters

	ITERATIONS  1000
    MCELL_DIFFUSION_CONSTANT_3D_anti 1e-3
    MCELL_DIFFUSION_CONSTANT_3D_rec 1e-3
    MCELL_DIFFUSION_CONSTANT_3D_B 1e-3

    MCELL_NO_COMPARTMENT_SIZE 1
    NA_um3 6.022e8
    VOL_RXN 1
    UNIMOL_RXN 10000
    MCELL_REDEFINE_VOL_RXN NA_um3
    
    kon1     10 * VOL_RXN
	koff1    1 * UNIMOL_RXN
	
    kon2     10 * VOL_RXN
	koff2    1 * UNIMOL_RXN

	koff3    5 * UNIMOL_RXN
	koff4    10 * UNIMOL_RXN
end parameters

begin species	
    anti(r,r)   200
    rec(a,b,b)  500
	B(c,p~U)    500
end species

begin reaction rules	
	rec(b)+B(c)     <->  rec(b!1).B(c!1)       kon1, koff1
	rec(a)+anti(r)  <->  rec(a!1).anti(r!1)    kon2, koff2
end reaction rules

begin observables
    Species Bc1pUantir2r3reca2b1bBc4pUreca3b5b4Bc5pU B(c!1,p~U).anti(r!2,r!3).rec(a!2,b!1,b).B(c!4,p~U).rec(a!3,b!5,b!4).B(c!5,p~U)
    Species Bc1pUantir2rreca2b1b3Bc3pU B(c!1,p~U).anti(r!2,r).rec(a!2,b!1,b!3).B(c!3,p~U)
    Species Bc1pUantir2rreca2b1b B(c!1,p~U).anti(r!2,r).rec(a!2,b!1,b)
    Species Bc1pUreca2b1bantir3r2reca3bb B(c!1,p~U).rec(a!2,b!1,b).anti(r!3,r!2).rec(a!3,b,b)
    Species Bc1pUreca2b3b1Bc3pUBc4pUantir5r2reca5b4b B(c!1,p~U).rec(a!2,b!3,b!1).B(c!3,p~U).B(c!4,p~U).anti(r!5,r!2).rec(a!5,b!4,b)
    Species Bc1pUreca2b3b1Bc3pUantir2r B(c!1,p~U).rec(a!2,b!3,b!1).B(c!3,p~U).anti(r!2,r)
    Species Bc1pUrecab2b1Bc2pU B(c!1,p~U).rec(a,b!2,b!1).B(c!2,p~U)
    Species BcpU B(c,p~U)
    Species antir1r2reca1bbBc3pUreca2b4b3Bc4pU anti(r!1,r!2).rec(a!1,b,b).B(c!3,p~U).rec(a!2,b!4,b!3).B(c!4,p~U)
    Species antir1rreca1bb anti(r!1,r).rec(a!1,b,b)
    Species antirr anti(r,r)
    Species reca1b2bBc2pUBc3pUantir4r1reca4b3b rec(a!1,b!2,b).B(c!2,p~U).B(c!3,p~U).anti(r!4,r!1).rec(a!4,b!3,b)
    Species reca1bbantir2r1reca2bb rec(a!1,b,b).anti(r!2,r!1).rec(a!2,b,b)
    Species recab1bBc1pU rec(a,b!1,b).B(c!1,p~U)
    Species recabb rec(a,b,b)

end observables
end model

generate_network({overwrite=>1})
simulate({method=>"ode",t_end=>1e-3,n_steps=>1000})
