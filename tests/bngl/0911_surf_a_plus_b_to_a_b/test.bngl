begin model

begin parameters
	#TIME_STEP  1e-6
	VACANCY_SEARCH_DISTANCE  10
	
	ITERATIONS  10
    MCELL_DIFFUSION_CONSTANT_2D_A 1e-6
    MCELL_DIFFUSION_CONSTANT_2D_B 5e-6
    
    MCELL_NO_COMPARTMENT_SIZE 0.0625
end parameters
begin molecule types
	A(a)
	B(b)
end molecule types

begin compartments
    PM 2 0.01     
    CP 3 0.1^3 PM  
end compartments

begin seed species
	 A(a)@PM  10
	 B(b)@PM  10
end seed species

begin observables
    Molecules A A
end observables

begin reaction rules
	A(a) + B(b) -> A(a!1).B(b!1) 1E9
end reaction rules

end model
