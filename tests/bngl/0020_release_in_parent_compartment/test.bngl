begin model

begin parameters
	ITERATIONS  10
    MCELL_DIFFUSION_CONSTANT_3D_A 1e-6
    MCELL_DIFFUSION_CONSTANT_3D_B 1e-6
    MCELL_DIFFUSION_CONSTANT_3D_C 1e-6 
    
    MCELL_NO_COMPARTMENT_SIZE 1
end parameters

begin molecule types
	A()
	B()
	C()
end molecule types

begin compartments
    EC 3 0.2^3
    PM 2 1e-20 EC     # mcell3r converter takes volume of PM into account
    CP 3 0.1^3 PM  
end compartments

begin seed species
	 A@EC  10
end seed species

begin observables
    Molecules A A
end observables

begin reaction rules
	A() + B() -> C() 1E9
end reaction rules

end model
