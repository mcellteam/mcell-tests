begin model
begin parameters
	ITERATIONS  50
	VACANCY_SEARCH_DISTANCE  10
    MCELL_DIFFUSION_CONSTANT_3D_A 1e-3
    MCELL_DIFFUSION_CONSTANT_3D_B 1e-3

    MCELL_NO_COMPARTMENT_SIZE 1
    NA_um3 6.022e8
    VOL_RXN 1
    MCELL_REDEFINE_VOL_RXN NA_um3
    
    k1on 1 * VOL_RXN * 20
    k1off 0.11  * 1000

end parameters

begin species
    A(x,x,b) 100
    B(a) 100
end species

begin reaction rules
    A(b) + B(a) <-> A(b!1).B(a!1) k1on, k1off
end reaction rules

begin observables
    Species Ba B(a)
end observables
end model

generate_network({overwrite=>1})
simulate({method=>"ode",t_end=>1e-3,n_steps=>1000})



