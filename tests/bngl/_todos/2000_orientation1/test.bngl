begin model

begin parameters
	#TIME_STEP  1e-6
	VACANCY_SEARCH_DISTANCE  10
	
	ITERATIONS  10
    MCELL_DIFFUSION_CONSTANT_3D_A 1e-6
    MCELL_DIFFUSION_CONSTANT_3D_B 5e-5
    MCELL_DIFFUSION_CONSTANT_3D_C 7e-5	
    
    MCELL_NO_COMPARTMENT_SIZE 0.0625
    
    rxn_layer_t  0.01
    vol_PM  0.6*rxn_layer_t 
  
end parameters
begin molecule types
	A()
	B()
	C()
end molecule types

begin compartments
  PM 2 vol_PM 
end compartments

begin seed species
	 A  10
	 B@PM  10
end seed species

begin observables
    Molecules A A
end observables

begin reaction rules
	A + B@CP@UP -> C 1E9
end reaction rules

end model
