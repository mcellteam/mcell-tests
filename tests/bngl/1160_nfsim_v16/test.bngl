# catalytic sythesis, dissociation and deletion
begin model
begin parameters
	ITERATIONS  100
    MCELL_DIFFUSION_CONSTANT_3D_A 1e-6
    MCELL_DIFFUSION_CONSTANT_3D_B 1e-6

    MCELL_DEFAULT_COMPARTMENT_VOLUME (1/8)^3
    NA_um3 6.022e8
    VOL_RXN 1
    UNIMOL_RXN 10000
    MCELL_REDEFINE_VOL_RXN NA_um3
    
    kon1     10 * UNIMOL_RXN
    kon2     10 * UNIMOL_RXN
    kon3     1 * UNIMOL_RXN
end parameters
begin species
	A(b)  0
	B(a)  200
end species
begin reaction rules
	B(a)           ->  B(a!1).A(b!1)    kon1
	B(a!1).A(b!1)  ->  B(a) + A(b)      kon2
	A(b)           ->  Null             kon3
	#A(b)           ->  0                kon3
end reaction rules
begin observables
    Molecules Dimer B(a!1).A(b!1)
    Molecules Free  A(b)
    Molecules Total A
end observables
end model

generate_network({overwrite=>1})
simulate({method=>"ode",t_end=>1e-3,n_steps=>1000})

