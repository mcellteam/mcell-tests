begin model

begin parameters
	ITERATIONS  10
  MCELL_DIFFUSION_CONSTANT_3D_A 1e-6
  MCELL_DIFFUSION_CONSTANT_3D_B 5e-5
  MCELL_DIFFUSION_CONSTANT_3D_C 7e-5	
  
  MCELL_DEFAULT_COMPARTMENT_VOLUME 0.0625^3
end parameters

begin molecule types
	A(x)
	B(a)
	C(a) # same components, even with the same name as B
end molecule types

begin seed species
	A(x!1).B(a!1)  10
end seed species

begin observables
  Molecules A A
  Molecules B B
  Molecules C C
end observables

begin reaction rules
	A(x!1).B(a!1) -> A(x!1).C(a!1) 1e5 #DeleteMolecules
end reaction rules

end model
