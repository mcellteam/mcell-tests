begin model
begin parameters
	ITERATIONS  1
	
  MCELL_DIFFUSION_CONSTANT_3D_A 1e-6
    
  MCELL_DEFAULT_COMPARTMENT_VOLUME 0.0625^3
end parameters

begin molecule types
	A(l,r,s~0~1)
end molecule types

begin seed species
	A(l,r!1,s~0).A(l!1,r!2,s~1).A(l!2,r,s~0)  10
	
end seed species

begin observables
	Species A2xs0 A(s~0).A(s~0)
end observables

begin reaction rules
  # just to make nfsim happy
	A(s~0) -> A(s~1) 0
end reaction rules

end model


generate_network({overwrite=>1})
simulate({method=>"ode",t_end=>1e-5,n_steps=>10})

