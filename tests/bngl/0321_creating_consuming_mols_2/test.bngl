begin model
begin parameters
	ITERATIONS  10
	#TIME_STEP  1e-6
	VACANCY_SEARCH_DISTANCE  10
	
    MCELL_DIFFUSION_CONSTANT_3D_A 1e-6
    MCELL_DIFFUSION_CONSTANT_3D_B 5e-5
    MCELL_DIFFUSION_CONSTANT_3D_C 1e-6
    MCELL_DIFFUSION_CONSTANT_3D_D 1e-6
        
    MCELL_NO_COMPARTMENT_SIZE 0.0625
end parameters

begin molecule types
	A(a~0~1)
	B
	C(c~2~3)
	D(d~4~5)
end molecule types

begin seed species
	 A(a~1)  10
	 B        0
	 C(c~2!1).D(d~4!1)       10
end seed species

begin observables
	Molecules _MDLString A, B
end observables

begin reaction rules
	
	A(a~1) + C(c~2!1).D(d~4!1) <-> A(a~0) + B   1e9,5e8
	   
end reaction rules
end model
