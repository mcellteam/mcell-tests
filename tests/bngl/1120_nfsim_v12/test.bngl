# simple_system.bngl
begin model
begin parameters

	ITERATIONS  100
    MCELL_DIFFUSION_CONSTANT_3D_A 1e-6
    MCELL_DIFFUSION_CONSTANT_3D_DNA 1e-6

    MCELL_DEFAULT_COMPARTMENT_VOLUME (1/8)^3
    NA_um3 6.022e8
    VOL_RXN 1
    UNIMOL_RXN 10000
    MCELL_REDEFINE_VOL_RXN NA_um3
    
    kon1     2 * VOL_RXN
	koff1    10 * UNIMOL_RXN
	
    #kon2     1 * VOL_RXN
	koff2    1 * UNIMOL_RXN

	koff3    5 * UNIMOL_RXN
	koff4    1 * UNIMOL_RXN
	koff5    1 * UNIMOL_RXN
end parameters
begin species
	A(b,p~U) 200
    A(b,p~P) 200
    DNA() 10
end species
begin reaction rules
	A(b,p~U) + A(b,p~P)    <->  A(b!1,p~U).A(b!1,p~P)    kon1, koff1
    #A(b!1,p~U).A(b!1,p~P)   ->  0                        koff2 # mcell3r converter does not handle the '0'
    A(b!1,p~U).A(b!1,p~P)   ->  Null                        koff2
    DNA()                   ->  DNA() + A(b,p~U)         koff3
    A(b,p~U)               <->  A(b,p~P)                 koff4, koff5
end reaction rules
begin observables
    Molecules Dimer A(b!1).A(b!1)
    Molecules DimerOnePhosOneNotPhos A(b!1,p~U).A(b!1,p~P)
    Species   SpecComplex A(b!+)
    Molecules Atotal A()
end observables
end model

generate_network({overwrite=>1})
simulate({method=>"ode",t_end=>1e-3,n_steps=>1000})


