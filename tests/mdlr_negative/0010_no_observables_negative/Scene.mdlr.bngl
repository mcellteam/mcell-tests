begin model
begin parameters
	ITERATIONS  1000
	TIME_STEP  1e-6
	VACANCY_SEARCH_DISTANCE  10
end parameters
begin molecule types
	A(a)
	B(b)
end molecule types
begin compartments
end compartments
begin seed species
	 A(a)  1
	 B(b)  1
end seed species
begin reaction rules
	A(a) + B(b) -> B(b!1).A(a!1) 1E5
end reaction rules
end model
