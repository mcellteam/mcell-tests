begin parameters
    MCELL_DIFFUSION_CONSTANT_2D_sa 1e-6
    MCELL_DIFFUSION_CONSTANT_2D_sb 1e-6
    MCELL_DIFFUSION_CONSTANT_3D_c 1e-6
end parameters

begin compartments
    PM 2 0
    Cube 3 0.125^3 PM
end compartments

begin seed species
    sa@PM 100
    sb@PM 100
end seed species  

begin reaction rules
    sa_plus_sb: sa + sb -> c@IN 1e2
end reaction rules  